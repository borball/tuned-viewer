#!/usr/bin/env python3
"""
Tuned Profile Viewer - Entry point script

This script allows running the tuned-viewer tool directly.
"""

import sys
import os

# Add the current directory to Python path so we can import tuned_viewer
sys.path.insert(0, os.path.dirname(os.path.abspath(__file__)))

from tuned_viewer.cli import main

if __name__ == '__main__':
    sys.exit(main())